generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["orderByAggregateGroup"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id      Int           @id @default(autoincrement())
  email   String        @unique
  name    String?
  ratings UserRatings[]
}

model UserRatings {
  id     Int     @id @default(autoincrement())
  rating Decimal
  user   User    @relation(fields: [userId], references: [id])
  userId Int
}
